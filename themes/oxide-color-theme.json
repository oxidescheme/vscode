	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#484848",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"constant.language",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#f67400"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric"],
			"settings": {
				"foreground": "#f67400"
			}
		},
		{
			"name": "Boolean",
			"scope": ["constant.language.boolean"],
			"settings": {
				"foreground": "#f67400"
			}
		},
		{
			"name": "String",
			"scope": ["string", "string.quoted", "string.template"],
			"settings": {
				"foreground": "#00c300"
			}
		},
		{
			"name": "String Escape",
			"scope": ["constant.character.escape", "constant.other.placeholder"],
			"settings": {
				"foreground": "#00baaa"
			}
		},
		{
			"name": "Regular Expression",
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#00baaa"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable", "variable.other", "variable.parameter"],
			"settings": {
				"foreground": "#00a6ff"
			}
		},
		{
			"name": "Variable Language (this, self, etc)",
			"scope": ["variable.language"],
			"settings": {
				"foreground": "#00a6ff",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variable Property",
			"scope": ["variable.other.property", "variable.other.object.property"],
			"settings": {
				"foreground": "#cecece"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#9588ff"
			}
		},
		{
			"name": "Storage (type, modifier, etc)",
			"scope": ["storage", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#9588ff"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator",
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.comparison",
				"keyword.operator.relational"
			],
			"settings": {
				"foreground": "#cecece"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call.generic"
			],
			"settings": {
				"foreground": "#ff5655"
			}
		},
		{
			"name": "Function Parameters",
			"scope": ["variable.parameter", "meta.parameters variable"],
			"settings": {
				"foreground": "#00a6ff"
			}
		},
		{
			"name": "Class",
			"scope": ["entity.name.class", "entity.name.type.class", "support.class"],
			"settings": {
				"foreground": "#00b3d6"
			}
		},
		{
			"name": "Type",
			"scope": ["entity.name.type", "support.type", "storage.type.primitive"],
			"settings": {
				"foreground": "#00b3d6"
			}
		},
		{
			"name": "Inherited Class",
			"scope": ["entity.other.inherited-class"],
			"settings": {
				"foreground": "#00b3d6"
			}
		},
		{
			"name": "Import/Include",
			"scope": [
				"keyword.control.import",
				"keyword.control.from",
				"keyword.control.export",
				"meta.import",
				"meta.require"
			],
			"settings": {
				"foreground": "#00baaa"
			}
		},
		{
			"name": "Preprocessor",
			"scope": ["meta.preprocessor", "entity.name.function.preprocessor"],
			"settings": {
				"foreground": "#00baaa"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#cecece"
			}
		},
		{
			"name": "Brackets",
			"scope": [
				"punctuation.section.brackets",
				"punctuation.section.parens",
				"punctuation.section.braces"
			],
			"settings": {
				"foreground": "#bebebe"
			}
		},
		{
			"name": "Tag",
			"scope": ["entity.name.tag", "meta.tag.sgml"],
			"settings": {
				"foreground": "#ff5655"
			}
		},
		{
			"name": "Tag Punctuation",
			"scope": ["punctuation.definition.tag"],
			"settings": {
				"foreground": "#bebebe"
			}
		},
		{
			"name": "Tag Attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#00a6ff"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.scss",
				"support.type.property-name.less"
			],
			"settings": {
				"foreground": "#cecece"
			}
		},
		{
			"name": "CSS Class/ID",
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#00b3d6"
			}
		},
		{
			"name": "CSS Selector",
			"scope": ["entity.name.tag.css"],
			"settings": {
				"foreground": "#ff5655"
			}
		},
		{
			"name": "JSON Key",
			"scope": [
				"support.type.property-name.json",
				"string.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#00a6ff"
			}
		},
		{
			"name": "Markdown Heading",
			"scope": ["markup.heading", "entity.name.section.markdown"],
			"settings": {
				"foreground": "#ff5655",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": ["markup.bold"],
			"settings": {
				"foreground": "#dedede",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": ["markup.italic"],
			"settings": {
				"foreground": "#cecece",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown Code",
			"scope": ["markup.inline.raw", "markup.fenced_code.block"],
			"settings": {
				"foreground": "#00baaa"
			}
		},
		{
			"name": "Markdown Link",
			"scope": ["markup.underline.link"],
			"settings": {
				"foreground": "#00a6ff"
			}
		},
		{
			"name": "Markdown Quote",
			"scope": ["markup.quote"],
			"settings": {
				"foreground": "#8f8f8f",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown List",
			"scope": ["punctuation.definition.list.begin.markdown"],
			"settings": {
				"foreground": "#9588ff"
			}
		},
		{
			"name": "Diff Inserted",
			"scope": ["markup.inserted", "markup.inserted.diff"],
			"settings": {
				"foreground": "#00c300"
			}
		},
		{
			"name": "Diff Deleted",
			"scope": ["markup.deleted", "markup.deleted.diff"],
			"settings": {
				"foreground": "#ff5655"
			}
		},
		{
			"name": "Diff Changed",
			"scope": ["markup.changed", "markup.changed.diff"],
			"settings": {
				"foreground": "#f67400"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#ff5655"
			}
		},
		{
			"name": "Invalid Deprecated",
			"scope": ["invalid.deprecated"],
			"settings": {
				"foreground": "#fb00ff"
			}
		}
	]
